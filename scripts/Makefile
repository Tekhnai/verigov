.PHONY: api-seed api-upgrade api-downgrade api-revision clean

# Proxy targets to repo root (allows running from scripts/)
api-seed:
	$(MAKE) -C .. api-seed

api-upgrade:
	$(MAKE) -C .. api-upgrade

api-downgrade:
	$(MAKE) -C .. api-downgrade

api-revision:
	$(MAKE) -C .. api-revision

clean:
	$(MAKE) -C .. clean
